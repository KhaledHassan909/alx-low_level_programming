#include "main.h"

/**
 * more_numbers - print numers 0-14 x10 times
 *
 * Return: None
 */
void more_numbers(void)
{
int n;
int iter;
while(iter <= 10)
{
n = 0;
while(n <= 14)
{
if(n < 10)
_putchar('0' + n);
else
{
_putchar('0' + n / 10);
_putchar('0' + n % 10);
}
n++;
}
_putchar('\n');
iter++;
}
}
