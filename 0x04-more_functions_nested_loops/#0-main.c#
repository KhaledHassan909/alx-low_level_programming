#include "main.h"
#include <stdio.h>


/**
 * mul - multiply two integers
 * @b: 2nd number
 *
 * Return: Product of a & b
 */

/**
 * main - check the code.
 * @a: 1st number
 *
 * Return: Always 0.
 */
int main(void)
{
    char c;

    c = 'A';
    printf("%c: %d\n", c, _isupper(c));
    c = 'a';
    printf("%c: %d\n", c, _isupper(c));
    return (0);
}
